<odoo>
    <record id="view_supplier_pricelist_list" model="ir.ui.view">
        <field name="name">supplier.pricelist.list</field>
        <field name="model">supplier.pricelist</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>
                <field name="supplier_id"/>
                <field name="price"/>
                <field name="available_qty"/>
                <field name="last_update"/>
            </list>
        </field>
    </record>

    <record id="view_supplier_pricelist_form" model="ir.ui.view">
        <field name="name">supplier.pricelist.form</field>
        <field name="model">supplier.pricelist</field>
        <field name="arch" type="xml">
            <form string="Prijslijst import">
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="supplier_id"/>
                        <field name="price"/>
                        <field name="available_qty"/>
                        <field name="last_update"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
